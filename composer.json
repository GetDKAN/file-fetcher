{
    "name": "getdkan/file-fetcher",
    "license": "GPL-3.0-only",
    "type": "library",
    "authors": [
        {
            "name": "fmizzell",
            "email": "fmizzell.dev@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4 <8.1",
        "ext-curl": "*",
        "getdkan/procrastinator": "^4.0.0",
        "guzzlehttp/guzzle": "^6.3",
        "rector/rector": "^0.15.0@dev"
    },
    "require-dev": {
        "getdkan/mock-chain": "dev-master",
        "mikey179/vfsstream": "^2.0@dev",
        "phpunit/phpunit": "^9.0",
        "squizlabs/php_codesniffer": "@stable"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "FileFetcher\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FileFetcherTests\\": "test/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "phpcbf": "./vendor/bin/phpcbf -p",
        "phpcs": "./vendor/bin/phpcs -ps",
        "phpunit": "./vendor/bin/phpunit",
        "phpunit-coverage": "./vendor/bin/phpunit --coverage-html ./coverage",
        "rector": "./vendor/bin/rector process",
        "rector-dry-run": "./vendor/bin/rector process --dry-run"
    }
}
